        {
            "name": "Depurador de Python: Archivo actual con argumentos",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "-f",
                "src\\main\\java\\Python\\julio.py",
                "-l",
                "-p",
                "-s"
            ]
        },
        {
            "type": "java",
            "name": "Depurador de Java: Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Depurador de Java: App",
            "request": "launch",
            "mainClass": "com.mycompany.ui_for_python/com.mycompany.ui_for_python.App",
            "projectName": "UI_for_Python"
        },
    ]
